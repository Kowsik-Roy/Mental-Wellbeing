{
  "version": 2,
  "builds": [
    {
      "src": "api/lambda.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "src": "/favicon\\.(ico|svg|png)",
      "dest": "/public/favicon.$1"
    },
    {
      "src": "/robots\\.txt",
      "dest": "/public/robots.txt"
    },
    {
      "src": "/audio/(.*)",
      "dest": "/public/audio/$1"
    },
    {
      "src": "/build/(.*)",
      "dest": "/public/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/lambda.php"
    }
  ]
}
